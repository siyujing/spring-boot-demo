<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.testingedu.springbootmybatisplus.mapper">

    <select id="selectByPrimaryKey" resultType="java.util.Map">
        SELECT
          id,
          name,
          pwd
        FROM
          demo_info
        LIMIT
          1
    </select>

    <select id="selectAll" resultType="java.util.HashMap">
        SELECT id,name,pwd,state FROM demo_info
    </select>

    <delete id="deleteAll" >
        delete from demo_info;
    </delete>

    <select id="selectDemoInfoList" resultType="DemoInfo">
        select * from demo_info where state = #{state}
    </select>

</mapper>